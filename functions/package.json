{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "emulators": "firebase emulators:start --only functions",
    "serve": "npm run build && firebase emulators:start --only functions",
    "serve:dev": "concurrently --kill-others \"npm run emulators\" \"npm run build:watch\"",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "deploy:api": "firebase deploy --only functions:api",
    "logs": "firebase functions:log",
    "dev": "npm run build -- --watch | firebase emulators:start --only functions"
  },
  "engines": {
    "node": "16"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@qualdesk/firestore-big-batch": "^0.0.2",
    "@sendinblue/client": "^3.1.0",
    "app-module-path": "^2.2.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "bwip-js": "^3.1.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.0",
    "express-handlebars": "^6.0.6",
    "fast-csv": "^4.3.6",
    "firebase": "^9.9.1",
    "firebase-admin": "^11.0.1",
    "firebase-functions": "^3.22.0",
    "handlebars": "^4.7.7",
    "html-pdf": "^3.0.1",
    "joi": "^17.6.0",
    "jsreport": "^3.5.0",
    "jsreport-chrome-pdf": "^1.10.0",
    "jsreport-handlebars": "^2.1.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "phantomjs-prebuilt": "^2.1.16",
    "qs": "^6.10.3",
    "uniqid": "^5.4.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/app-module-path": "^2.2.0",
    "@types/body-parser": "^1.19.2",
    "@types/bwip-js": "^3.0.0",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/html-pdf": "^3.0.0",
    "@types/joi": "^17.2.3",
    "@types/jsreport": "^2.9.1",
    "@types/module-alias": "^2.0.1",
    "@types/qs": "^6.9.7",
